import time
timestamp=(time.strftime('%H:%M:%S'))
print(timestamp)
hour = int(time.strftime('%H'))


if hour <= 12:
  print("Good Morning Sir!")

elif hour > 12 and hour < 18 :
  print("Good Afternoon Sir!")

elif hour >= 18 and hour < 20:
  print("Good Eve Sir!")

elif hour >= 20:
  print("Good Night Sir!")

else:
  print("Have a good day sir!")